{
  "name": "@rill/preset-react-native",
  "version": "0.1.0",
  "type": "module",
  "description": "React Native preset for rill - default components and EngineView",
  "author": "kookyleo <kookyleo@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
